'><span class="k">def</span> <span class="nf">exception_hook</span><span class="p">(</span><span class="nb">type</span><span class="p">,</span> <span class="n">value</span><span class="p">,</span> <span class="n">tback</span><span class="p">):</span>&#x000A;</div><div class='line' id='LC304'>    <span class="c1"># log the exception here</span>&#x000A;</div><div class='line' id='LC305'>    <span class="k">print</span><span class="p">(</span><span class="s">'Exception hook triggered.'</span><span class="p">)</span>&#x000A;</div><div class='line' id='LC306'>    <span class="k">print</span><span class="p">(</span><span class="nb">type</span><span class="p">)</span>&#x000A;</div><div class='line' id='LC307'>    <span class="k">print</span><span class="p">(</span><span class="n">value</span><span class="p">)</span>&#x000A;</div><div class='line' id='LC308'>    <span class="k">print</span><span class="p">(</span><span class="n">tback</span><span class="p">)</span>&#x000A;</div><div class='line' id='LC309'>    <span class="c1"># then call the default handler</span>&#x000A;</div><div class='line' id='LC310'>    <span class="n">sys</span><span class="o">.</span><span class="n">__excepthook__</span><span class="p">(</span><span class="nb">type</span><span class="p">,</span> <span class="n">value</span><span class="p">,</span> <span class="n">tback</span><span class="p">)</span>&#x000A;</div><div class='line' id='LC311'>&#x000A;</div><div class='line' id='LC312'>&#x000A;</div><div class='line' id='LC313'><span class="k">if</span> <span class="n">__name__</span> <span class="o">==</span> <span class="s">"__main__"</span><span class="p">:</span>&#x000A;</div><div class='line' id='LC314'>    <span class="n">sys</span><span class="o">.</span><span class="n">excepthook</span> <span class="o">=</span> <span class="n">exception_hook</span>&#x000A;</div><div class='line' id='LC315'>    <span class="k">try</span><span class="p">:</span>&#x000A;</div><div class='line' id='LC316'>        <span class="n">main</span><span class="p">()</span>&#x000A;</div><div class='line' id='LC317'>    <span class="k">except</span> <span class="nb">Exception</span> <span class="k">as</span> <span class="n">e</span><span class="p">:</span>&#x000A;</div><div class='line' id='LC318'>        <span class="k">print</span><span class="p">(</span><span class="s">'Error =&gt;'</span><span class="p">,</span> <span class="n">e</span><span class="p">)</span>&#x000A;</div><div class='line' id='LC319'>        <span class="k">print</span><span class="p">(</span><span class="s">'Error =&gt;'</span><span class="p">,</span> <span class="n">traceback</span><span class="o">.</span><span class="n">format_exc</span><span class="p">())</span>&#x000A;</div><div class='line' id='LC320'>        <span class="nb">exit</span><span class="p">()</span>&#x000A;</div><div class='line' id='LC321'>    <span class="k">finally</span><span class="p">:</span>&#x000A;</div><div class='line' id='LC322'>        <span class="k">pass</span>&#x000A;</div><div class='line' id='LC323'>&#x000A;</div><div class='line' id='LC324'>&#x000A;</div><div class='line' id='LC325'>&#x000A;</div><div class='line' id='LC326'>&#x000A;</div><div class='line' id='LC327'>&#x000A;</div><div class='line' id='LC328'>&#x000A;</div><div class='line' id='LC329'>&#x000A;</div><div class='line' id='LC330'>&#x000A;</div><div class='line' id='LC331'>&#x000A;</div><div class='line' id='LC332'>&#x000A;</div><div class='line' id='LC333'>&#x000A;</div><div class='line' id='LC334'>&#x000A;</div><div class='line' id='LC335'>&#x000A;</div><div class='line' id='LC336'>&#x000A;</div><div class='line' id='LC337'>&#x000A;</div><div class='line' id='LC338'>&#x000A;</div><div class='line' id='LC339'>&#x000A;</div><div class='line' id='LC340'>&#x000A;</div><div class='line' id='LC341'>&#x000A;</div><div class='line' id='LC342'>&#x000A;</div><div class='line' id='LC343'>&#x000A;</div><div class='line' id='LC344'>&#x000A;</div><div class='line' id='LC345'>