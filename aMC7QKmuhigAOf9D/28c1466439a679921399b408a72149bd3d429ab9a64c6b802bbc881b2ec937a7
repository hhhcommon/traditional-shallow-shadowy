<script>
  var title_import_url = "https://github.com/phpDocumentor/phpDocumentor2.git";
  var title_post_url = "/mirrors/phpDocumentor2/update_import";
  var title_fork_url = "/mirrors/phpDocumentor2/sync_fork";
