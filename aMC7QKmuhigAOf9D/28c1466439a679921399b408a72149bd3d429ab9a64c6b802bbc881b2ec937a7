      wepayUrl: '/mirrors/phpDocumentor2/wepay',
      nameIsBlank: '名称不能为空',
      nameTooLong: '名称过长（最多为 36 个字符）',
      modalHelper: Gitee.modalHelper
    });
  
    if ("" === 'true') {
