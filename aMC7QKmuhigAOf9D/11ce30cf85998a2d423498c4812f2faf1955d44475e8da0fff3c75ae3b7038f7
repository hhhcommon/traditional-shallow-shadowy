      $(this).select();
    });
  
    $('.spread').on('click', function() {
      var prev = $(this).prev();
      var text = prev.text();
      prev.text(prev.attr('src_text')).attr('src_text', text);
      text = $(this).text();
      $(this).text($(this).attr('src_text')).attr('src_text', text);
    });
  })
</script>


