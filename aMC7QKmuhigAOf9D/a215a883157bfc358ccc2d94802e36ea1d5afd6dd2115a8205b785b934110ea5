    new ProjectInfoEditor({
      el: '.git-project-desc-wrapper',
      homepage: "https://blog.igeekfan.cn",
      description: "A simple and practical CMS implemented by .NET Core 3.1",
      url: "/supercoeus/lin-cms-dotnetcore/update_description",
      modalHelper: Gitee.modalHelper
    })
  })
  
  if (false) {
    gon.project_new_blob_path = "/supercoeus/lin-cms-dotnetcore/new/master"
    bindShowModal({
      el: $('.no-license .project-license__create'),
      complete: function(data, modal) {
        if (!data.haveNoChoice && !data.data) {
          Flash.show('请选择一项开源许可证')
        } else {
          location.href = gon.project_new_blob_path + '?license=' + data.data
        }
      },
      skip: function () {
        location.href = gon.project_new_blob_path + '?license'
      }
    })
  }
  
  $('i.help.circle.icon').popup({
    popup: '.no-license .ui.popup',
    position: 'right center'
  })
  
  $('#remove-no-license-message').on('click', function(){
    $.cookie("skip_repo_no_license_message_8014323", 'hide', { expires: 365 })
    $('#user-no-license-message').hide()
    return
  })
</script>
