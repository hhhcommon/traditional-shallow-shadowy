<script>
  $(function () {
    Gitee.initTabsInDropdown($('#git-project-branch').dropdown({
      fullTextSearch: true,
      onChange: function (value, text) {
        var path = $('#path').val();
        var href = ['/mirrors/vn-py/tree', encodeURIComponent(value), path].join('/');
        window.location.href = href;
      }
    }));
    $('.protected-branch-popup').popup()
  })
</script>

