<script>
  var $proType = $('.pro-type');
  var $proLanguage = $('.pro-language');
  
  $proType.dropdown({
    onChange: function(type) {
      var langId = $proLanguage.length > 0 ? $proLanguage.dropdown('get value') : '';
      $.ajax({
        type: 'GET',
        url: "/organizations/APIJSON/projects",
        data: {
          type: type,
          lang_id: langId,
          search: $('#search-projects').val()
        }
      });
    }
  });
  
  $proLanguage.dropdown('set selected', ).dropdown({
    onChange: function(id) {
      var type = $proType.length > 0 ? $proType.dropdown('get value') : '';
      $.ajax({
        type: 'GET',
        url: "/organizations/APIJSON/projects",
        data: {
          lang_id: id,
          type: type,
          search: $('#search-projects').val()
        }
      });
    }
  })
</script>

