<script>
    $('#search_project .icon-search').click(function() {
      getSearchResult();
    })
  
  $('#search-projects').on('keydown', function(e) {
    if (e.keyCode == 13) {
      getSearchResult();
    }
  })
  
  function getSearchResult() {
    var searchContent = $('#search-projects').val();
    var langId = $('.pro-language').length > 0 ? $('.pro-language').dropdown('get value') : ''
    var type = $('.pro-type').length > 0 ? $('.pro-type').dropdown('get value') : ''
    $.ajax({
      type: 'GET',
      url: "/organizations/APIJSON/projects",
      dataType: 'script',
      beforeSend: function() {
        $('#search_project').find('.ui.icon.input').addClass('loading')
      },
      complete: function() {
        $('#search_project').find('.ui.icon.input').removeClass('loading')
      },
      data: {
        search: searchContent,
        lang_id: langId,
        type: type
      }
    })
  }
</script>

