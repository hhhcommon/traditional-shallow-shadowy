<script>
  (function() {
    var switch_order_card;
  
    if (false) {
      switch_order_card = function(index) {
        var $card, $item;
        $item = $('.title-item').eq(index);
        if ($item) {
          $item.addClass('active').siblings().removeClass('active');
        }
        $card = $('.order-card').eq(index);
        if ($card) {
          return $card.addClass('active').siblings().removeClass('active');
        }
      };
      $('.order-card-title').on('touchend', '.title-item', function() {
        return switch_order_card($(this).index());
      });
      $('.order-cards').on('touchend', '.panel', function() {
        var $active_index, index;
        $active_index = $('.title-item.active').index();
        if ($(this).hasClass('r')) {
          index = $active_index + 1;
          return switch_order_card(index > 4 ? 0 : index);
        } else {
          index = $active_index - 1;
          return switch_order_card(index < 0 ? 4 : index);
        }
      });
    }
  
  }).call(this);
</script>

