{
    /* æ¿€æ´»æ’ä»¶æ–¹æ³• */
    public static function activate()
    {
        //æŒ‚è½½è¯„è®ºæ¥å£
        Typecho_Plugin::factory('Widget_Feedback')-&gt;finishComment = array('CommentToDingtalk_Plugin', 'send');

        return 'æ’ä»¶å·²æ¿€æ´»,è¯·è®¾ç½®ç›¸å…³ä¿¡æ¯';
    }

    /* ç¦ç”¨æ’ä»¶æ–¹æ³• */
    public static function deactivate()
    {
        return 'æ’ä»¶å·²ç¦ç”¨';
    }

    /* æ’ä»¶é…ç½®æ–¹æ³• */
    public static function config(Typecho_Widget_Helper_Form $form)
    {
        $webhook = new Typecho_Widget_Helper_Form_Element_Text('webhook', null, '', 'Webhookåœ°å€', 'è¯·å°†é’‰é’‰ä¸­çš„webhookåœ°å€å¡«åˆ°æ­¤å¤„');
        $secret  = new Typecho_Widget_Helper_Form_Element_Text('secret', null, '', 'Secretå¯†é’¥', 'è¯·å°†é’‰é’‰ä¸­çš„Secretå¯†é’¥å¡«åˆ°æ­¤å¤„');
        $form-&gt;addInput($webhook);
        $form-&gt;addInput($secret);
    }

    /**
     * ä¸ªäººç”¨æˆ·çš„é…ç½®é¢æ¿
     *
     * @access public
     * @param Typecho_Widget_Helper_Form $form
     * @return void
     */
    public static function personalConfig(Typecho_Widget_Helper_Form $form)
    {

    }

    /* æ’ä»¶å®ç°æ–¹æ³• */
    public static function render()
    {

    }

    /* æ¨é€é€šçŸ¥æ–¹æ³• */
    public static function send($post)
    {
        //è·å–ç³»ç»Ÿé…ç½®
        $options = Helper::options();
        //åˆ¤æ–­æ˜¯å¦é…ç½®webhookåœ°å€
        if (is_null($options-&gt;plugin('CommentToDingtalk')-&gt;webhook)) {
            throw new Typecho_Plugin_Exception(_t('Webhookåœ°å€æœªé…ç½®'));
        }
        //åˆ¤æ–­æ˜¯å¦é…ç½®secretå¯†é’¥
        if (is_null($options-&gt;plugin('CommentToDingtalk')-&gt;secret)) {
            throw new Typecho_Plugin_Exception(_t('Secretå¯†é’¥æœªé…ç½®'));
        }
        $webhook = $options-&gt;plugin('CommentToDingtalk')-&gt;webhook;
        $secret  = $options-&gt;plugin('CommentToDingtalk')-&gt;secret;
        $text = &quot;###æ‚¨æ”¶åˆ°äº†ä¸€æ¡æ–°è¯„è®º\næ–‡ç« æ ‡é¢˜ï¼š{$post-&gt;title}\nè¯„è®ºä½œè€…ï¼š{$post-&gt;author}\nè¯„è®ºå†…å®¹ï¼š{$post-&gt;text}&quot;;
        $data = [
            'msgtype' =&gt; 'actionCard',
            'actionCard' =&gt; [
                'title' =&gt; 'æ‚¨æ”¶åˆ°äº†ä¸€æ¡æ–°è¯„è®º',
                'text' =&gt; $text,
                'btnOrientation' =&gt; 1,
                'hideAvatar' =&gt; 0,
                'singleTitle' =&gt; 'æŸ¥çœ‹è¯¦æƒ…',
                'singleURL' =&gt;$post-&gt;permalink
            ]
        ];
        
        //æˆ‘ä¿®æ”¹äº†è¿™é‡Œ
        //æˆ‘ä¿®æ”¹äº†è¿™é‡Œ
        //æˆ‘ä¿®æ”¹äº†è¿™é‡Œ
        $response = (new self())-&gt;request((new self())-&gt;getWebhook($webhook,$secret), json_encode($data));
        //æˆ‘ä¿®æ”¹äº†è¿™é‡Œ
        //æˆ‘ä¿®æ”¹äº†è¿™é‡Œ
        //æˆ‘ä¿®æ”¹äº†è¿™é‡Œ

        if($response['errcode'] !== 0){
            //å‘é€å¤±è´¥ï¼Œè®°å½•æ—¥å¿—
            $log = @file_get_contents('./error.log');
            file_put_contents('./error.log','['.date(&quot;Y-m-d H:i:s&quot;).']'.$response['errmsg']);
        }
    }

    /* Curlè¯·æ±‚ç²¾ç®€ç‰ˆ */
    private function request($url, $postData)
    {
        $ch = curl_init();
        curl_setopt($ch, CURLOPT_URL, $url);
        curl_setopt($ch, CURLOPT_POST, 1);
        curl_setopt($ch, CURLOPT_CONNECTTIMEOUT, 5);
        curl_setopt($ch, CURLOPT_HTTPHEADER, array('Content-Type: application/json;charset=utf-8'));
        curl_setopt($ch, CURLOPT_POSTFIELDS, $postData);
        curl_setopt($ch, CURLOPT_RETURNTRANSFER, true);
        // çº¿ä¸‹ç¯å¢ƒä¸ç”¨å¼€å¯curlè¯ä¹¦éªŒè¯, æœªè°ƒé€šæƒ…å†µå¯å°è¯•æ·»åŠ è¯¥ä»£ç 
        // curl_setopt ($ch, CURLOPT_SSL_VERIFYHOST, 0);
        // curl_setopt ($ch, CURLOPT_SSL_VERIFYPEER, 0);
        $data = curl_exec($ch);
        curl_close($ch);
        return json_decode($data, true);
    }

    /* webhookåœ°å€ */
    private function getWebhook($webhook, $secret)
    {
        list($msec, $sec) = explode(' ', microtime());
        $timestamp        = (float) sprintf('%.0f', (floatval($msec) + floatval($sec)) * 1000);
        $sign             = $this-&gt;signSecret($timestamp, $secret);
        $webhook_url      = &quot;{$webhook}&amp;timestamp={$timestamp}&amp;sign={$sign}&quot;;
        return $webhook_url;
    }

    /* è®¡ç®—ç­¾å */
    private function signSecret($timestamp,$secret){
        $stringToSign = $timestamp.&quot;\n&quot;.$secret;
        return urlencode(base64_encode(hash_hmac('sha256', $stringToSign, $secret, true)));
    }
}</code></pre>            </div>
