<script>
  $('#search-projects').on('keydown', function(e) {
    if(e.keyCode == 13) {
      var searchContent = $(this).val()
      $.ajax({
        type: 'GET',
        url: "/langxj/watched",
        dataType: 'script',
        beforeSend: function() {
          $('#search_project').find('.ui.icon.input').addClass('loading')
        },
        complete: function() {
          $('#search_project').find('.ui.icon.input').removeClass('loading')
        },
        data: {
          search: searchContent
        }
      });
    }
  })
</script>

