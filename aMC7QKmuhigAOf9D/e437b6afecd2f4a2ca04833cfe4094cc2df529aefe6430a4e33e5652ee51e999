<script>
  $('#article-action-useful, #article-action-useless').click(function() {
    $.ajax({
      type: 'post',
      url: "/help/judge_article",
      data: {
        article_id: "4166",
        useful: this.id == 'article-action-useful' ? true : false
      },
      success: function(res) {
        if (res.data === true) {
          $('#article-action-useful').removeClass('basic secondary').addClass('green');
          $('#article-action-useless').removeClass('pink').addClass('basic secondary');
        } else {
          $('#article-action-useless').removeClass('basic secondary').addClass('pink');
          $('#article-action-useful').removeClass('green').addClass('basic secondary');
        }
      },
      error: function(res) {
        Flash.show(res.responseJSON.message);
        if (res.status == 401)
          window.location.href = "/login";
      }
    });
  })
  
  $('#article-action-collect').click(function() {
    $.ajax({
      type: 'post',
      url: "/help/collect_article",
      data: { article_id: "4166" },
      success: function(res) {
        if(res.data === true)
          $('#article-action-collect').removeClass('basic');
        else
          $('#article-action-collect').addClass('basic');
      },
      error: function(res) {
        Flash.show(res.responseJSON.message);
        if (res.status == 401)
          window.location.href = "/login";
      }
    });
  })
</script>

</div>
</div>
</div>


