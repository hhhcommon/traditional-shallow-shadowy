<script>
  $(function () {
    var $detailMenu = $('.explore-languagues__detail-menu')
    var $showBtn = $('.explore-languagues .show-more')
  
    $showBtn.on('click', function () {
      if ($detailMenu.hasClass('hide')) {
        $detailMenu.removeClass('hide')
        $showBtn.text('收起')
      } else {
        $detailMenu.addClass('hide')
        $showBtn.text('更多语言')
      }
    })
  
    if ($detailMenu.has('.active').length) {
      $showBtn.trigger('click')
    }
  })
</script>

