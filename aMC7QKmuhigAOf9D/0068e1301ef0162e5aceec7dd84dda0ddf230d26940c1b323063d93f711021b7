      alertText: '提示',
      okText: '确定'
    });
  
    donateModal = new ProjectDonateModal({
      el: '#project-donate-modal',
      alipayUrl: '/hkq15/Besking/alipay',
      wepayUrl: '/hkq15/Besking/wepay',
      nameIsBlank: '名称不能为空',
